    "naropa_marpa": {
      "p1": "གདམས་པ་དེ་དག་ཏི་ལོ་པ་ནས་ཁོང་གི་སློབ་མ་གྲུབ་ཆེན་ནཱ་རོ་པ་ (༡༠༡༦-༡༡༠༠) ལ་བརྒྱུད་ཅིང་། ནཱ་རོ་ཆོས་དྲུག་ཏུ་གཏན་ལ་ཕབ། སྒོམ་སྒྲུབ་ཀྱི་རིམ་པ་འདི་དག་ནི་བཀའ་བརྒྱུད་པའི་སྙིང་པོའི་བསྟན་པར་རྩི།",
      "p2": "ནཱ་རོ་པས་ཁོང་གི་གདམས་པ་རྣམས་ལོ་ཙཱ་བ་ཆེན་པོ་མར་པ་ཆོས་ཀྱི་བློ་གྲོས་ (༡༠༡༢-༡༠༩༧) ལ་གནང་། ཁོང་ནི་གདམས་པ་ཞུ་བའི་ཆེད་དུ་བོད་ནས་རྒྱ་གར་ལ་ཕེབས་ཤིང་། དེ་ནས་བོད་དུ་ཕྱིར་ཕེབས་ཏེ་དམ་པའི་ཆོས་ཀྱི་བསྟན་པ་རྒྱ་ཆེར་སྤེལ།",
      "p3": "མར་པའི་སློབ་མ་གཙོ་བོ་ནི་རྗེ་བཙུན་མི་ལ་རས་པ་ (༡༠༤༠-༡༡༢༣) ཡིན། ཁོང་ནི་བོད་ཀྱི་རྣལ་འབྱོར་པ་ཆེན་པོ་ཞིག་ཏུ་གྱུར། ཁོང་གི་ཡབ་སྔ་སར་གྲོངས་པའི་རྐྱེན་གྱིས་སྐུ་ངལ་ཆེན་པོ་བྱུང་བ་དང་། ཨ་ཁུ་དང་ཨ་ནེ་ལ་མཐུ་བཏང་ནས་དགྲ་ཤ་བླངས་པ། ཕྱིས་སུ་འགྱོད་པ་དྲག་པོས་དམ་པའི་ཆོས་ལ་ཞུགས་འདོད་སྐྱེས་པ་སོགས་ཁོང་གི་ཐུན་མོང་མ་ཡིན་པའི་རྣམ་ཐར་ནི་བོད་མི་རྣམས་ཀྱིས་མཁྱེན་གསལ་ལྟར་ཡིན།",
      "p4": "ཁོང་གིས་དཀའ་སྤྱད་སྙིང་རུས་དང་རྐྱེན་ངན་ལམ་དུ་ཁྱེར་བའི་ནུས་པར་བརྟེན་ནས་ཆོས་ཉིད་ཀྱི་གནས་ལུགས་མངོན་སུམ་དུ་རྟོགས། ཁོང་གི་མགུར་འབུམ་སོགས་གསུང་རྩོམ་རྣམས་ད་ལྟའི་བར་དུ་གནས་ཡོད།"
    },
    "gampopa_phagdru": {
      "p1": "མི་ལ་རས་པའི་བསྟན་པ་ནི་སྒམ་པོ་པ་ (༡༠༧༩-༡༡༥༣) འམ་དྭགས་པོ་ལྷ་རྗེས་མུ་མཐུད་དེ་འཛིན་སྐྱོང་སྤེལ་གསུམ་གནང་། ཁོང་གིས་ཐོག་མར་བཀའ་གདམས་པའི་ལུགས་སུ་སློབ་གཉེར་གནང་སྟེ་རིམ་པ་དང་རྒྱུད་རིམ་ལྡན་པའི་ལམ་ལ་ཞུགས། ཕྱིས་སུ་མི་ལ་རས་པ་མཇལ་ཞིང་དེའི་མདུན་དུ་ཉམས་ལེན་གནང་བས་གདམས་པ་ཡོངས་རྫོགས་ཀྱི་དོན་ངོ་མ་ཐུགས་སུ་ཆུད།",
      "p2": "དེ་ནས་བཟུང་བརྒྱུད་པ་འདི་ལ་དྭགས་པོ་བཀའ་བརྒྱུད་ཅེས་གྲགས། སྒམ་པོ་པ་ལས་བཀའ་བརྒྱུད་ཆེ་བཞི་སྟེ་ཀརྨ་བཀའ་བརྒྱུད་དང་། ཚལ་པ་བཀའ་བརྒྱུད། འབའ་རོམ་བཀའ་བརྒྱུད། ཕག་གྲུ་བཀའ་བརྒྱུད་བཅས་བྱུང་།",
      "p3": "ཕག་གྲུ་བཀའ་བརྒྱུད་ཀྱི་སྲོལ་གཏོད་པ་པོ་ནི་ཕག་མོ་གྲུ་པ་རྡོ་རྗེ་རྒྱལ་པོ་ (༡༡༡༠-༡༡༧༠) ཡིན་ལ། ཁོང་ནི་སྒམ་པོ་པའི་སློབ་མ་གཙོ་བོ་ཞིག་ཡིན། ཁོང་གི་གདུང་རྒྱུད་ཀྱིས་ཆོས་སྡེ་བསྐྱངས་པ་མེད་པར་གྱུར་ནའང་། ཁོང་གི་རིགས་རྒྱུད་ཀྱིས་དེའི་རྗེས་ཀྱི་དུས་རབས་ནང་བོད་ཀྱི་སྲིད་དབང་སྐྱོང་བའི་ཐད་གལ་ཆེའི་་མཛད་རྗེས་བཞག ཕག་མོ་གྲུ་པའི་སློབ་མ་གཙོ་བོ་རྣམས་ཀྱིས་རང་རང་གི་བརྒྱུད་པ་བཙུགས་པ་ལས་ད་ལྟའི་བར་དུ་གནས་པ་ནི་འབྲི་གུང་བཀའ་བརྒྱུད་དང་། སྟག་ལུང་བཀའ་བརྒྱུད། འབྲུག་པ་བཀའ་བརྒྱུད་བཅས་གསུམ་པོ་འདི་ཡིན།"
    },
    "origin_drikung": {
      "title": "འབྲི་གུང་བཀའ་བརྒྱུད་ཀྱི་འབྱུང་ཁུངས།"
    },
    "jigten_sumgon": {
      "p1": "ཕག་མོ་གྲུ་པའི་ཐུགས་སྲས་སྐྱོབ་པ་འཇིག་རྟེན་གསུམ་མགོན་ (༡༡༤༣-༡༢༡༧) གྱིས་བླ་མ་སྐུ་གཤེགས་པའི་རྗེས་སུ་ལོ་གསུམ་རིང་ (༡༡༧༧-༡༡༧༩) གདན་ས་མཐིལ་གྱི་གདན་ས་བསྐྱངས། དེ་ནས་ཕག་མོ་གྲུ་པའི་ལུང་བསྟན་པ་ལྟར་འབྲི་གུང་དུ་འབྲི་གུང་མཐིལ་དགོན་པ་ཕྱག་བཏབ་སྟེ་རང་ལུགས་ཀྱི་སྲོལ་བཏོད།",
      "p2": "ཕག་མོ་གྲུ་པར་སློབ་མ་གྲངས་ལས་འདས་པ་ཡོད་ནའང་། འཇིག་རྟེན་གསུམ་མགོན་ནི་ཐུགས་སྲས་གཙོ་བོ་ཞིག་ཡིན། ཕག་མོ་གྲུ་པས་ས་བཅུ་ཐོབ་པའི་བྱང་ཆུབ་སེམས་དཔའ་ཞིག་གིས་བསྟན་པ་དང་བྱིན་རླབས་འཛིན་པར་འགྱུར་ཞེས་ལུང་བསྟན་གནང་། འཇིག་རྟེན་གསུམ་མགོན་གྱིས་ཕག་མོ་གྲུ་པའི་མདུན་ནས་བསྟན་པ་ཡོངས་རྫོགས་དང་། གསང་བའི་མན་ངག་གི་བརྒྱུད་པ། བཤད་ཁྲིད་དང་དབང་། རྟོགས་པའི་བྱིན་རླབས་སོགས་ཞུས། དེ་ནས་འཇིག་རྟེན་གསུམ་མགོན་གྱིས་བསྟན་པ་ཡོངས་རྫོགས་ཁོང་གི་སློབ་མ་གཙོ་བོ་གུ་ར་བ་ཚུལ་ཁྲིམས་རྡོ་རྗེ་ (༡༡༥༤-༡༢༢༡) ལ་གནང་།",
      "p3": "སྐྱོབ་པ་འཇིག་རྟེན་གསུམ་མགོན་ནི་སྐྱུ་རའི་གདུང་རྒྱུད་ལས་བྱུང་། བོད་བརྒྱུད་ནང་བསྟན་གྱི་ཆོས་བརྒྱུད་མང་པོའི་ལུགས་སྲོལ་ལྟར། འཇིག་རྟེན་གསུམ་མགོན་གྱིས་རང་གི་གཉེན་ཚན་ཁྲོད་ནས་རྒྱུད་འཛིན་བདམས་གནང་མཛད། དེར་བརྟེན་ཐོག་མར་འབྲི་གུང་གདན་རབས་གསུམ་མ་གཏོགས་གཞན་རྣམས་སྐྱུ་རའི་གདུང་རྒྱུད་ཀྱི་སྲས་ལས་བྱུང་བ་ཤ་སྟག་ཡིན་མོད། འོན་ཀྱང་ནང་མིའི་ཁྲོད་ནས་གདན་ས་འཛིན་པར་དམིགས་བསལ་གྱི་ཁྲིམས་སྲོལ་གཏན་འབེབས་མེད། འབྲི་གུང་དུ་ཐོག་མ་ཉིད་ནས་ཆོས་སྲིད་ཀྱི་དབང་ཆ་སོ་སོར་བགོས་ཡོད་དེ། ཆོས་ཕྱོགས་ཀྱི་དབུ་ཁྲིད་ནི་གདན་རབས་ཀྱིས་མཛད་ཅིང་། སྲིད་ཕྱོགས་ཀྱི་ལས་དོན་རྣམས་སྒོམ་པས་སྐྱོང་གི་ཡོད། དེ་གཉིས་ཀ་སྤྱིར་བཏང་སྐྱུ་རའི་རིགས་རྒྱུད་ནས་ཡིན།",
      "p4": "བརྒྱུད་པ་འདིའི་དུས་འགོར་འཇིག་རྟེན་གསུམ་མགོན་གྱི་སློབ་མ་རྒྱལ་བ་ལྷ་ནང་པ་ (༡༡༦༤-༡༢༢༤) ལས་ལྷ་པ་བཀའ་བརྒྱུད་ཅེས་པའི་ཡན་ལག་ཅིག་བྱུང་ཡང། དེ་ནི་ཡུན་རིང་གནས་ཐུབ་མེད།"
    },
    "early_developments": {
      "title": "བསྟན་པའི་དུས་འགོའི་དར་རྒྱས། (༡༢༡༧-༡༤༠༠)",
      "p1": "ཐོག་མའི་འབྲི་གུང་གདན་རབས་འགའ་ཞིག་ལ་སྙན་གྲགས་ཆེན་པོ་བྱུང་། དེའི་ནང་ནས་སྤྱན་སྔ་གྲགས་པ་འབྱུང་གནས་ (༡༡༧༥-༡༢༥༥) ནི་འཇིག་རྟེན་གསུམ་མགོན་གྱི་ཐུགས་སྲས་ཡིན་ཞིང་། ཁོང་ལ་ཕག་མོ་གྲུ་པའི་གདན་ས་དགོན་གྱི་དབུ་ཁྲིད་མཛད་རྒྱུའི་འགན་བསྩལ། ཁོང་གིས་དགོན་པ་དེ་ཉིད་དར་རྒྱས་ཀྱི་དུས་རབས་གསར་པ་ཞིག་ཏུ་ཁྲིད་ཅིང་། ༡༢༣༥ ལོར་འབྲི་གུང་གི་གདན་ས་བཟུང་། ཁོང་གིས་ཞི་འཇམ་དང་བརྟན་པོའི་སྒོ་ནས་དུས་ཚོད་དཀའ་ངལ་ཅན་གྱི་སྐབས་སུ་བསྟན་པ་བསྐྱངས། བོད་མི་རྣམས་ཀྱིས་སོག་པོའི་དབང་བསྒྱུར་ལ་ངོ་རྒོལ་བྱས་ཏེ་ཁྲལ་འཇལ་རྒྱུ་མཚམས་འཇོག་སྐབས། ཇིང་གེར་རྒྱལ་པོའི་ (༡༡༦༢-༡༢༢༧) སྲས་དང་ཤུལ་འཛིན་པ་ཨོ་གོ་དེའི་ (༡༡༨༦-༡༢༤༡) ཡིས་ ༡༢༣༩ ལོར་དྲག་གནོན་གྱི་བྱ་འགུལ་འཇིགས་སུ་རུང་བ་ཞིག་སྤེལ་རྒྱུའི་བཀའ་བཏང་།",
      "p2": "སོག་དམག་གིས་དགོན་སྡེ་མང་པོ་མེར་བསྲེགས་བཏང་བའི་ནང་རྭ་སྒྲེང་དགོན་པ་ཚུད་ཡོད་ཅིང་། དེར་ཁོང་ཚོས་དགེ་འདུན་པ་ ༥༠༠ བཀྲོངས་ནས་ཉེ་འགྲམ་གྱི་འབྲི་གུང་ཕྱོགས་སུ་དཔུང་བསྐྱོད། འབྲི་གུང་གི་སྲིད་སྐྱོང་པས་ཁོང་ཚོར་རྒོལ་ལན་སྤྲད་ཀྱང་བཟུང་གཅུན་བྱས་ཏེ་སྲོག་ཐོག་ཏུ་གཏོང་ལ་ཁད་དུ་ཡོད། གྲགས་པ་འབྱུང་གནས་འབྲི་གུང་མཐིལ་ནས་སོག་དམག་གི་ཕྱོགས་སུ་མགྱོགས་པོར་ཕེབས་སྐབས་གནམ་ནས་རྡོ་ཆར་བབས་ཞེས་བཤད་སྲོལ་ཡོད། གཅིག་བྱས་ན་དེ་ནི་སེར་བ་དྲག་པོ་ཞིག་ཡིན་སྲིད། གང་ལྟར་ཡང་། ཁོང་གིས་བློ་སྟོབས་ཆེན་པོས་ཞུ་གཏུགས་གནང་བ་དེས་སོག་པོ་དང་ལྷན་དུ་ཞི་བའི་གྲོས་མོལ་བྱུང་། རྭ་སྒྲེང་དགོན་པར་ཚུར་རྒོལ་དྲག་པོ་བྱས་པ་དེ་ནི་འབྲི་གུང་ལ་ཉེན་བརྡ་གཏོང་ཆེད་ཡིན་པ་གསལ་པོར་མངོན། འོན་ཀྱང་འབྲི་གུང་ནི་མཇུག་འབྲས་ངེས་མེད་ཀྱི་ དམག་འཁྲུག་ལས་ཐར་ཏེ། བོད་དབུས་ཁུལ་དུ་སྟོབས་ཤུགས་ཆེ་བའི་གྲས་སུ་གྱུར།",
      "p3": "ས་སྐྱ་པཎྜི་ཏ་ཀུན་དགའ་རྒྱལ་མཚན་ (༡༡༨༢-༡༢༥༡) གྱིས་ཨོ་གོ་དེའི་དམག་དཔོན་གོ་དན་དང་ལྷན་དུ་ཆིངས་ཡིག་བཞག་སྟེ། བོད་ཀྱིས་སོག་པོའི་མངའ་འོག་ཏུ་སྡོད་རྒྱུ་དང་ཁྲལ་འཇལ་རྒྱུར་ཁས་ལེན་བྱས། ཀུན་དགའ་རྒྱལ་མཚན་གྱིས་ཁྱིམ་མཚེས་སྟོབས་ཆེན་དང་ལྷན་དུ་བཞག་པའི་ཐག་གཅོད་དེ་ལ་རང་ཡུལ་དུ་རྒྱབ་སྐྱོར་གཅིག་གྱུར་མ་ཐོབ། བོད་ཀྱི་སྐུ་དྲག་མང་པོས་ཁྲལ་གྱི་དགོས་འདུན་ལ་ངོ་རྒོལ་བྱས། སོག་དམག་གིས་ ༡༢༥༡ ལོར་ཁོང་ཚོའི་ངོ་རྒོལ་དེ་ཁྲག་དྲི་བྲོ་ཞིང་ཕམ་ཉེས་ཚབས་ཆེན་བཏང་ནས་མགོ་བཅག་པར་བྱས།",
      "p4": "ཀུན་དགའ་རྒྱལ་མཚན་གྱི་ཚ་བོ་དང་ཤུལ་འཛིན་པ་འཕགས་པ་བློ་གྲོས་ (༡༢༣༥-༡༢༨༠) ནི་ ༡༢༥༩ ནས་བཟུང་སོག་པོའི་རྒྱལ་པོ་ཆེན་པོ་ཁུབ་ལའི་སེ་ཆེན་དང་། ཕྱིས་སུ་རྒྱལ་པོ་ཆེན་པོ་ཁུབ་ལའི་ (༡༢༡༥-༡༢༩༤) ཡི་དབུ་བླར་གྱུར། ཁོང་ཚོ་སོག་པོ་དང་འབྲེལ་བ་ཟབ་མོ་ཡོད་པས་ས་སྐྱ་པས་བོད་ནང་ཁུལ་དུ་ཆབ་སྲིད་ཀྱི་དབུ་ཁྲིད་བྱེད་ཐུབ་པ་བྱུང་། གང་ལགས་ཤེ་ན། ཀརྨ་བཀའ་བརྒྱུད་པས་སོག་པོའི་རྒྱབ་སྐྱོར་འཚོལ་བའི་འདུན་པར་ལམ་ལྷོང་མ་བྱུང་། ༡༢༧༥ ལོར་ཁུབ་ལའི་ཧན་གྱིས་འཕགས་པ་བློ་གྲོས་ལ་དབུས་གཙང་གི་ཞིང་ཆེན་ཁག་དང་། ཁམས་དང་ཨ་མདོ་བཅས་བقطོས་བཀོད་བརྒྱུད་འཛིན་གྱི་ས་ཁོངས་སུ་ཕུལ། ཞིང་ཆེན་དེ་དག་ཁྲི་སྐོར་དུ་བགོས་ཡོད་པ་སྟེ། ཁྲི་སྐོར་རེར་མིང་ཙམ་གྱི་ཆ་ནས་ཁྱིམ་ཚང་ ༡༠,༠༠༠ རེ་ཚུད་པའི་འཛིན་སྐྱོང་ས་ཁུལ་ཡིན། ཁུབ་ལའི་ ༡༢༧༩ ལོར་རྒྱ་ནག་གི་གོང་མ་གྱུར་ནས་ཡོན་རྒྱལ་རབས་བཙུགས་སྐབས། ཁོང་གིས་འཕགས་པ་བློ་གྲོས་ལ་གོང་མའི་དབུ་བླ་ཞེས་པའི་མཚན་གནས་ཕུལ། ཐབས་ལམ་འདིའི་ཐོག་ནས་བོད་དུ་ཐེངས་དང་པོར་གོང་མ་དང་དབུ་བླའི་བར་གྱི་མཆོད་ཡོན་འབྲེལ་བའི་ཐོག་ནས་ཆོས་སྲིད་ཟུང་འབྲེལ་གྱི་གཞུང་གི་ངོ་བོ་གྲུབ།",
      "p5": "རྒྱལ་ཁབ་འདི་བقطོས་བཀོད་བརྒྱུད་འཛིན་གྱི་ས་ཁུལ་དུ་བསྒྱུར་བར་ངོ་རྒོལ་བྱེད་མཁན་བོད་ཀྱི་སྐུ་དྲག་ཁྲོད་དུ། ལྷག་པར་དུ་འབྲི་གུང་དང་། ཕག་གྲུ། ཀརྨ་བཀའ་བརྒྱུད་བཅས་དང་འབྲེལ་བ་དམ་ཟབ་ཡོད་པའི་བོད་དབུས་ཁུལ་གྱི་དབུས་དང་གཙང་གི་ཞིང་ཆེན་ཁག་ཏུ་འགོ་ཚུགས། དུས་མཚུངས་སུ་སོག་པོའི་ཚོ་ཁག་ཕན་ཚུན་བར་དབུ་ཁྲིད་དང་ས་ཞིང་བདག་དབང་ཐད་འཐབ་རྩོད་བྱེད་བཞིན་ཡོད། དུས་སྐབས་དེར་འབྲི་གུང་སྟོབས་ཤུགས་ཆེན་པོར་གྱུར། གདན་རབས་ ༥ པ་གཅུང་རྡོ་རྗེ་གྲགས་པའི་ (༡༢༡༠-༡༢༧༨) སྐབས་སུ་ཡང་འབྲི་གུང་དགོན་པར་དགེ་འདུན་པ་མང་པོ་འདུས་པའི་གནས་ཚུལ་ཡོད། ༡༢༥༩ ལོར་རྒྱལ་པོ་ཆེན་པོ་མོང་ཁེ་གྲོངས་རྗེས། ཁུབ་ལའི་ཡི་སྤུན་དང་ཁ་གཏད་ཧུ་ལ་གུ་ (ཧ་ལམ་ ༡༢༡༧-༡༢༦༥) འབྲི་གུང་བཀའ་བརྒྱུད་པའི་སྦྱིན་བདག་ཏུ་གྱུར་ཅིང་། ཁོང་གིས་སོག་དམག་གི་རུ་ཁག་ཆུང་ངུ་ཞིག་ཁོང་ཚོའི་བདེ་འཇགས་ཆེད་འབྲི་གུང་དུ་བཏང་།",
      "p6": "འོན་ཀྱང་མུར་སོག་པོའི་ཤོག་ཁག་ཕན་ཚུན་བོད་ཀྱི་ས་ཐོག་ཏུ་འཐབ་རྩོད་བྱས་པ་དེ་ཕྱིས་སུ་ལོ་རྒྱུས་སུ་འབྲི་གུང་དང་ས་སྐྱའི་བར་གྱི་དམག་འཁྲུག་ཅེས་གྲགས་པ་དེ་བྱུང་། དེ་ནི་ཆོས་ལུགས་ཀྱི་དམག་འཁྲུག་གམ། ཡང་ན་བོད་བརྒྱུད་ནང་བསྟན་གྱི་གྲུབ་མཐའ་ཁག་གཉིས་བར་གྱི་འཁོན་འཛིན་ཞིག་མིན་པར། དེ་ལས་ལྡོག་སྟེ་བོད་དབུས་ཁུལ་གྱི་ཞིང་ཆེན་འགའ་ཞིག་གིས་ཁ་གཏད་སོག་པོའི་རྒྱབ་སྐྱོར་འོག་ཁུབ་ལའི་ཡིས་ཁོང་ཚོར་བཙན་དབང་བཏང་བའི་བདག་དབང་ལ་ངོ་རྒོལ་བྱེད་པའི་ཐབས་ཤེས་ཤིག་ཡིན། ས་གནས་ཀྱི་དོན་དངོས་གདོང་ཐུག་ནི་ཕག་མོ་གྲུ་པའི་གདན་ས་མཐིལ་དགོན་པའི་གདན་རབས་བརྒྱུད་འཛིན་ཐད་ཀྱི་རྩོད་གླེང་ལ་གཙོ་བོར་དམིགས་པ་ཡིན། འདིར་རླངས་ལྷ་གཟིགས་ཀྱི་གདུང་རྒྱུད་ཀྱི་ཡན་ལག་སོ་སོས་རང་རང་གི་ཁེ་ཕན་མི་མཐུན་པ་རྣམས་སྒྲུབ་ཐབས་བྱེད་ཀྱི་ཡོད། ཡན་ལག་གཅིག་ལ་ས་སྐྱ་པས་རྒྱབ་སྐྱོར་དང་། གཞན་དེར་འབྲི་གུང་པས་རྒྱབ་སྐྱོར་བྱས། དམག་འཁྲུག་དེ་གདན་རབས་ ༧ པ་ཙམ་སྤྱས་གྲགས་པ་བསོད་ནམས་ (༡༢༣༨-༡༢༨༦) ཀྱི་སྐབས་སུ་ལངས། ༡༢༩༠ ལོར་ཁོང་གི་རྒྱུད་འཛིན་པ་སྣུབས་ཇོ་སྒོ་རྡོ་རྗེ་ཡེ་ཤེས་ (༡༢༢༣-༡༢༩༣) ཀྱི་སྐབས་སུ་སོག་དམག་གིས་འབྲི་གུང་མཐིལ་དགོན་པ་རྩ་གཏོར་བཏང་། གོད་ཆག་འདི་ནི་ས་གནས་མི་མང་ལ་མཚོན་ན་བསམ་ཡུལ་ལས་འདས་པ་ཞིག་མིན། རྡོ་རྗེ་ཡེ་ཤེས་ནི་སྐྱུ་རའི་གདུང་རྒྱུད་མིན་པར་སྣུབས་ཀྱི་རིགས་ཡིན་པས། ཁོང་ལ་དཀའ་ངལ་ཅན་གྱི་དུས་སུ་རྒྱབ་སྐྱོར་མ་ཐོབ་སྟེ། གདུང་རྒྱུད་ཕྱི་མི་ཞིག་གིས་འབྲི་གུང་གི་གདན་ས་འཛིན་པ་ནི་བརྒྱུད་པ་འདིའི་སྲོལ་གཏོད་པའི་ཐུགས་དགོངས་དང་མི་མཐུན་པར་མཐོང་བའི་ལྟ་ཚུལ་ཁྱབ་ཆེ་བས་སོ།",
      "p7": "འབྲི་གུང་མཐིལ་ནི་གདན་རབས་ ༩ པ་ཅུང་རིན་པོ་ཆེ་རྡོ་རྗེ་རིན་ཆེན་ (༡༢༧༨-༡༣༡༤) གྱི་སྐབས་སུ་གོང་མ་དང་ས་སྐྱ་པའི་རྒྱབ་སྐྱོར་འོག་བསྐྱར་བཞེངས་མཛད། འོན་ཀྱང་དགོན་པས་བོད་དུ་ལོ་བཅུ་ཕྲག་མང་པོར་ཤུགས་རྐྱེན་ཆེན་པོ་བཞག་པ་དེ་མེད་པར་གྱུར། རྡོ་རྗེ་རིན་ཆེན་གྱིས་དགོན་པའི་འཆད་ཉན་ཆེད་ལོ་རེའི་དུས་ཚིགས་གསུམ་གྱི་སྒྲིག་གཞི་གསར་པ་བཞག",
      "p8": "དཔྱིད་ཀ་དང་སྟོན་ཀར་གཙོ་བོ་བརྒྱུད་པའི་གཞུང་ཆེན་ཁག་དང་ཐེག་པ་ཆེན་པོའི་རྨང་གཞིའི་ཆོས་རྣམས་འཆད་ཁྲིད་དང་། དབྱར་ཁ་ཕྱག་རྒྱ་ཆེན་པོ་ལྔ་ལྡན་གྱི་ཁྲིད་གནང་། དགུན་ཁ་རྡོ་རྗེ་རིན་ཆེན་གྱིས་རས་གོས་སྲབ་མོ་ཙམ་བཞེས་ནས་འབྲི་གུང་མཐིལ་གྱི་གཙུག་ལག་ཁང་གི་ཕྱི་རོལ་གྱི་ཁྱམས་ར་ཆེན་པོའི་སྟེང་ནཱ་རོ་ཆོས་དྲུག་གི་ཁྲིད་གནང་།",
      "p9": "དུས་རབས་ ༡༤ པའི་ནང་། ཕག་མོ་གྲུ་པས་བོད་དབུས་ཁུལ་གྱི་སྲིད་དབང་བཟུང་། འདིར་ཉམས་རྒུད་དུ་འགྲོ་བཞིན་པའི་མིང་མཚུངས་པའི་བོད་བརྒྱུད་ནང་བསྟན་གྱི་གྲུབ་མཐའ་དེར་གོ་བ་མིན་པར། ཕག་གྲུ་བཀའ་བརྒྱུད་དང་འབྲེལ་བ་དམ་ཟབ་ཡོད་པའི་རླངས་ཀྱི་གདུང་རྒྱུད་ཀྱི་སྲིད་སྐྱོང་པ་རྣམས་ལ་གོ དུས་རབས་དེའི་མི་སྣ་གཙོ་བོ་ནི་བྱང་ཆུབ་རྒྱལ་མཚན་ (༡༣༠༢-༡༣༧༣) ཡིན་ཞིང་། ཁོང་ནི་ཆབ་སྲིད་མཁས་པ་ཞིག་དང་ཕག་གྲུ་བཀའ་བརྒྱུད་ཀྱི་དགེ་སློང་ཞིག་ཀྱང་ཡིན། ཁོང་གིས་རྒྱལ་ཁབ་དེ་རང་བཙན་ཆེ་བའི་ཕྱོགས་སུ་ཁྲིད་ཅིང་འཛིན་སྐྱོང་གི་ལམ་ལུགས་གསར་པ་བཏོད། ཁོང་གི་བཅོས་སྒྱུར་གྱིས་འབྲི་གུང་དང་འབྲེལ་བ་ཇེ་སྡུག་ཏུ་བཏང་སྟེ་ཕག་མོ་གྲུ་པ་དང་དྲག་པོའི་གདོང་ཐུག་བྱུང་། ཐོག་མའི་རྒྱལ་ཁའི་རྗེས་སུ། འབྲི་གུང་པ་རྣམས་གཡུལ་སར་ཕམ་ཉེས་ཚབས་ཆེན་འགའ་དང་ལེན་བྱེད་དགོས་བྱུང་བ་དང་། དབུས་བྱང་ཕྱོགས་སུ་ཕག་མོ་གྲུ་པའི་དབང་བསྒྱུར་དང་མཐུན་ཐབས་བྱེད་དགོས་བྱུང་། བྱང་ཆུབ་རྒྱལ་མཚན་གྱི་མི་ཚེའི་མཇུག་ཏུ། འབྲི་གུང་པས་ཁོང་ཚོའི་ས་ཁུལ་གྱི་རང་སྐྱོང་དབང་ཆ་སླར་གསོ་བྱེད་ཐུབ་པ་བྱུང་། དུས་སྐབས་དེ་ཙམ་ལ་ཡོན་རྒྱལ་རབས་ཉམས་པ་དང་མིང་རྒྱལ་རབས་ (༡༣༦༨-༡༦༤༤) རྒྱ་ནག་གི་གོང་མར་གྱུར་པ་བཅས་བརྒྱུད་ཨེ་ཤི་ཡའི་ནང་དབང་ཆའི་་ལྗིད་ཚད་ལ་འགྱུར་བ་འགྲོ་བཞིན་ཡོད། འོན་ཀྱང་སོག་པོ་རྣམས་ད་དུང་བོད་པ་དང་མིང་གོང་མ་གཉིས་ཀས་མཉམ་དུ་འབྲེལ་གཏུག་བྱེད་དགོས་པའི་སྟོབས་ཤུགས་ཤིག་ཆགས་ཡོད།",
      "p10": "ཟང་ཟིང་དེ་དག་གི་ཁྲོད་དུ། གདན་རབས་ ༡༡ པ་སྤྱན་སྔ་ཆོས་ཀྱི་རྒྱལ་པོས་ (༡༣༣༥-༡༤༠༧) འབྲི་གུང་གི་བསྟན་པའི་གནས་ཚད་སླར་ཡང་གོང་མཐོར་གཏོང་ཐབས་གནང་། ཁོང་གིས་སྣར་ཐང་གི་བཀའ་འགྱུར་དང་གསར་དུ་ཞུས་དག་བྱས་པའི་བསྟན་འགྱུར་པར་བཤུས་བྱེད་དུ་བཅུག དགེ་ལུགས་པའི་སྲོལ་གཏོད་པ་པོ་རྗེ་ཙོང་ཁ་པ་ (༡༣༥༧-༡༤༡༩) ༡༣༧༣ ལོར་ཨ་མདོ་ནས་འབྲི་གུང་དུ་ཕེབས་ཤིང་། ཆོས་ཀྱི་རྒྱལ་པོའི་སློབ་མར་གྱུར། ཁོང་གི་ནང་མི་རྣམས་དགོན་པ་དང་ལེ་དབར་ ༤༠ ཙམ་གྱི་སར་ཡོད་པའི་གྲོང་གསེབ་ཅིག་ཏུ་གཞིས་ཆགས། ཙོང་ཁ་པས་འབྲི་གུང་ལུགས་ཀྱི་ནཱ་རོ་ཆོས་དྲུག་གི་ཁྲིད་དང་། འཇིག་རྟེན་གསུམ་མགོན་གྱི་ཕྱི་ནང་གི་གསུང་རྩོམ་ཡོངས་རྫོགས་གསན་བསམ་མཛད།"
    },
    "reform": {
      "title": "ཉམས་གསོའི་དུས་རབས། (༡༤༠༠-༡༦༡༥)",
      "p1": "དུས་རབས་ ༡༥ པའི་ནང་། མིང་རྒྱལ་རབས་ཀྱིས་འབྲི་གུང་པའི་ཤུགས་རྐྱེན་སླར་གསོ་དང་ཇེ་ཆེར་འགྲོ་བཞིན་པ་ངོས་འཛིན་བྱས་ཏེ། གདན་རབས་ལ་ཆན་ཅའོ་ཝང་སྟེ་ཆོས་རྗེའི་མཚན་གནས་ཕུལ་བ་དེ་ནི་གྲུབ་མཐའ་དང་དགོན་པ་གལ་ཆེ་ཤོས་བརྒྱད་ཀྱི་དབུ་ཁྲིད་ལ་སྤྲོད་སྲོལ་ཡོད། དུས་རབས་གཉིས་ཀྱི་རིང་གཙང་གི་གཞིས་ཀ་རྩེ་དེ་བོད་ཀྱི་དབང་ཆའི་ལྟེ་གནས་སུ་གྱུར་ཏེ། ཐོག་མར་རིན་སྤུངས་པ་ (༡༤༣༦-༡༥༦༦) དང་། དེའི་རྗེས་སུ་གཙང་པ་ (༡༥༦༦-༡༦༤༢) བཅས་ཀྱིས་དབང་བསྒྱུར་བྱས། སྐུ་དྲག་གི་ཁྱིམ་རྒྱུད་དེ་གཉིས་ཀས་ཀརྨ་པར་རྒྱབ་སྐྱོར་བྱས་པས། ཀརྨ་བཀའ་བརྒྱུད་པ་དེ་དུས་སྐབས་དེའི་ཤུགས་རྐྱེན་ཆེ་ཤོས་ཀྱི་གྲུབ་མཐའ་ཞིག་ཏུ་གྱུར།",
      "p2": "དུས་རབས་ ༡༦ པའི་འགོ་སྟོད་དུ་འབྲི་གུང་གི་གདན་སར་བྱོན་པའི་སྐྱེས་བུ་དམ་པ་གཉིས་ཀྱིས་མཛད་རྗེས་ཆེན་པོ་བཞག་ཡོད། དེ་ནི་རྒྱལ་དབང་ཀུན་དགའ་རིན་ཆེན་ (༡༤༧༥-༡༥༢༧) དང་ཁོང་གི་རྒྱལ་ཚབ་རྒྱལ་དབང་རིན་ཆེན་ཕུན་ཚོགས་ (༡༥༠༩-༡༥༥༧) གཉིས་ཡིན། ཀུན་དགའ་རིན་ཆེན་ནི་འཇིག་རྟེན་གསུམ་མགོན་གྱི་རྣམ་སྤྲུལ་དུ་ངོས་འཛིན་བྱེད། ཁོང་གིས་ཐུགས་དམ་ཉམས་བཞེས་ཀྱི་སྤུས་ཚད་ཇེ་ལེགས་སུ་གཏོང་ཆེད་ཐུགས་རེ་གནང་། ཀུན་དགའ་རིན་ཆེན་གྱིས་དབང་ལུང་འཆད་ཁྲིད་གནང་རྒྱུར་སྐུ་ངལ་བསྐྱོན་པ་དང་། སྒྲུབ་མཚམས་ཀྱི་སྲོལ་རྒྱུན་སླར་གསོ་གནང་རྒྱུར་ཡང་ཐུགས་ཁུར་ཆེན་པོ་བཞེས། ཁོང་གི་མདུན་དུ་འབྲི་གུང་ལ་འདུས་པའི་སློབ་མ་གསར་པ་མང་པོ་གངས་ཏི་སེ་དང་། རྩྭ་རི། ལ་ཕྱི་བཅས་སུ་སྒྲུབ་མཚམས་ལ་བཏང་། ཁོང་གི་བཀོད་ཁྱབ་འོག་འབྲི་གུང་མཐིལ་དགོན་དུ་སྒྲུབ་ཁང་གསར་པ་ ༥༠ བཞེངས། བཀའ་འགྱུར་དང་བསྟན་འགྱུར་གཉིས་མཐིང་ཤོག་ཐོག་གསེར་དངུལ་གྱིས་བྲིས་ཤིང་། འབྲི་གུང་བརྒྱུད་པའི་གསུང་རབ་ཡོངས་རྫོགས་བཞེངས་སྐྲུན་ཆེད་ཡིག་མཁན་ཉིས་བརྒྱ་ཙམ་ཞུགས།",
      "p3": "འབྲི་གུང་གདན་རབས་ ༡༧ པ་རིན་ཆེན་ཕུན་ཚོགས་ནི་བསྟན་པ་ཉམས་གསོ་མཛད་མཁན་རླབས་ཆེན་ཞིག་ཡིན། བརྒྱུད་པ་མི་འདྲ་བ་ཁག་ནས་གདམས་པ་མང་པོ་ཞུས་རྗེས། ཁོང་གིས་ལྟ་གྲུབ་དང་། ཆོ་ག སྒོམ་སྒྲུབ་ཁག་ལྷག་པར་དུ་རྙིང་མ་པའི་ལུགས་རྣམས་འབྲི་གུང་བཀའ་བརྒྱུད་ཀྱི་ཐུན་མོང་མ་ཡིན་པའི་བསྟན་པའི་ནང་གཅིག་ཏུ་བསྒྲིལ་བར་མཛད་པས། དེས་དགོངས་པའི་ཕྱོགས་ལྷུང་མེད་པར་རྒྱ་བསྐྱེད་དང་ཡར་རྒྱས་བཏང་། རིན་ཆེན་ཕུན་ཚོགས་ཀྱིས་གཏེར་སྒྲོམ་ལུང་པའི་ཀི་རི་ཡང་རྫོང་ནས་ཟབ་གཏེར་དགོངས་པ་ཡང་ཟབ་གཏེར་ནས་བཞེས། རིན་ཆེན་ཕུན་ཚོགས་ནི་རྩོམ་པ་པོ་བརྩོན་འགྲུས་ཅན་ཞིག་ཡིན་ལ། ཁོང་གི་གསུང་རྩོམ་རྣམས་རྙིང་མ་པས་ཀྱང་ཚད་མཐོན་པོར་འཛིན་བཞིན་ཡོད་པ་དང་རྙིང་མའི་རྒྱུད་འབུམ་གྱི་ཁོངས་སུ་ཡང་བསྡུས་ཡོད།",
      "p4": "རིན་ཆེན་ཕུན་ཚོགས་ཀྱི་སྲས་གཅིག་པུ་ཆོས་རྒྱལ་རིན་ཆེན་ཕུན་ཚོགས་ (༡༥༤༧-༡༦༠༢) ཀྱིས་འབྲི་གུང་གདན་རབས་ ༢༡ པ་མཛད་སྐབས། ཐུ་མེད་སོག་པོའི་དབུ་ཁྲིད་སྟོབས་ཆེན་ཨལ་ཐན་རྒྱལ་པོས་ (༡༥༠༧-༡༥༨༢) དགེ་ལུགས་པའི་བླ་མ་བསོད་ནམས་རྒྱ་མཚོ་ (༡༥༤༣-༡༥༨༨) དང་མཆོད་ཡོན་གྱི་འབྲེལ་བ་བཙུགས་པ་དེས་མ་འོངས་པའི་བོད་ཀྱི་ལོ་རྒྱུས་ལ་ཤུགས་རྐྱེན་ཆེན་པོ་ཐེབས་ཡོད། སོག་པོའི་རྒྱལ་པོས་བསོད་ནམས་རྒྱ་མཚོར་ཏཱ་ལའི་བླ་མ་ཞེས་པའི་མཚན་གནས་ཕུལ་ཞིང་ཐོབ་ཐང་རྒྱ་ཆེན་པོ་སྤྲད། བསོད་ནམས་རྒྱ་མཚོ་ནི་ཏཱ་ལའི་བླ་མ་སྐུ་ཕྲེང་གསུམ་པ་ཡིན་ཏེ། ཁོང་གི་སྐུ་གོང་མ་གཉིས་ལ་འདས་རྗེས་སུ་ཏཱ་ལའི་བླ་མའི་མཚན་གནས་ཕུལ་བ་ཡིན། དུས་རབས་ ༡༦ པའི་ལོ་རབས་ཕྱི་མའི་ནང་དམག་འཁྲུག་གི་ཟང་ཟིང་མང་པོ་བྱུང་བའི་རྐྱེན་གྱིས། ཆོས་རྒྱལ་རིན་ཆེན་ཕུན་ཚོགས་ཀྱིས་འབྲི་གུང་རྫོང་གི་རྒྱ་ཁྱོན་ཆེ་རུ་བཏང་སྟེ་མཁར་རྫོང་ཞིག་ཏུ་བསྒྱུར།",
      "p5": "བརྒྱུད་པའི་ནང་གོ་གནས་མཐོ་ཤོས་ཀྱི་གདུང་རྒྱུད་བརྒྱུད་འཛིན་གྱི་དུས་རིམ་དང་པོ་དེ་ཆོས་རྒྱལ་རིན་ཆེན་ཕུན་ཚོགས་ཀྱི་སྲས་རྣམས་ཀྱི་ཐོག་ནས་མཇུག་རྫོགས། ཁོང་གི་སྲས་རྒན་པ་ནཱ་རོ་བཀྲ་ཤིས་ཕུན་ཚོགས་ (༡༥༧༤-༡༦༢༨) སམ་ནཱ་རོ་གཉིས་པར་གྲགས་པ་དེས་གདན་ས་བཟུང་། སྲས་འབྲིང་བ་མགར་དབང་ཆོས་ཀྱི་དབང་ཕྱུག་ (༡༥༨༤-༡༦༣༠) ནི་ཞྭ་དམར་དྲུག་པར་ངོས་འཛིན་ཞུས། སྲས་ཆུང་བ་གཉིས་ཏེ་རྒྱལ་དབང་དཀོན་མཆོག་རིན་ཆེན་ (༡༥༩༠-༡༦༥༤) དང་ཀུན་མཁྱེན་རིག་འཛིན་ཆོས་གྲགས་ (༡༥༩༥-༡༦༥༩) གཉིས་ནི་འབྲི་གུང་གདན་སའི་གདུང་རྒྱུད་ཐ་མ་དེ་གྱུར་ཏེ། སྐྱུ་རའི་གདུང་རྒྱུད་ཁོང་གཉིས་ཀྱི་ཐོག་ནས་ཆད། དཀོན་མཆོག་རིན་ཆེན་དགོངས་པ་རྫོགས་རྗེས་འབྲི་གུང་པས་གདན་རབས་ཀྱི་ཡང་སྲིད་འཚོལ་འགོ་བཙུགས། གཅུང་པོ་ཆེ་ཚང་དང་ཆུང་ཚང་ཞེས་བརྒྱུད་འཛིན་རྣམ་གཉིས་ཀྱི་ལམ་ལུགས་གསར་དུ་བཏོད། འབྲི་གུང་གདན་རབས་ལོ་རྒྱུས་ནང་དཀོན་མཆོག་རིན་ཆེན་ནི་ཆེ་ཚང་དང་པོ་དང་། རིག་འཛིན་ཆོས་གྲགས་ནི་ཆུང་ཚང་དང་པོ་ཡིན་པར་བཞེད། རྣམ་གཉིས་ཀས་འབྲི་གུང་སྐྱབས་མགོན་གྱི་མཚན་གནས་བཞེས།"
    },
    "chetsang_chungtsang": {
      "title": "ཆེ་ཚང་དང་ཆུང་ཚང་རིན་པོ་ཆེའི་དུས་རབས། (༡༦༡༥ ནས་ད་ལྟའི་བར།)",
      "p1": "ཆེ་ཚང་རིན་པོ་ཆེ་སྐུ་ཕྲེང་དང་པོ་དཀོན་མཆོག་རིན་ཆེན་ (༡༥༩༠-༡༦༥༥) གྱི་སྐབས་སུ། ཏཱ་ལའི་བླ་མ་སྐུ་ཕྲེང་བཞི་པ་ནི་ཐུ་མེད་གདུང་རྒྱུད་ཀྱི་རིགས་ཡིན་པས། ཐུ་མེད་སོག་པོ་རྣམས་ཀྱིས་བོད་ཀྱི་ས་ཞིང་དང་རྒྱུ་དངོས་ལ་བདག་དབང་གི་ཐོབ་ཐང་རྩོད། དེའི་རྐྱེན་གྱིས་འབྲི་གུང་པ་རྣམས་སླར་ཡང་དམག་འཁྲུག་གི་ནང་དུ་ཚུད་ཅིང་། སོག་དམག་གིས་དཔུང་འཇུག་དྲག་པོ་བྱས་ཏེ་འབྲི་གུང་རྫོང་གི་མཁར་རྫོང་ཕམ་ཉེས་སུ་བཏང་། འབྲི་གུང་གི་ས་ཁུལ་ཡོངས་རྫོགས་གྱང་གོག་ཏུ་གྱུར་པས་དཀོན་མཆོག་རིན་ཆེན་ས་གནས་དེར་དུས་ཡུན་རིང་པོར་བཞུགས་མ་ཐུབ་པར་གྱུར། གཙང་པའི་རྒྱལ་པོ་ཕུན་ཚོགས་རྣམ་རྒྱལ་གྱིས་སོག་པོ་རྣམས་ཕྱིར་འབུད་བྱེད་ཐུབ་པར་གྱུར་རྗེས། དཀོན་མཆོག་རིན་ཆེན་གྱིས་ ༡༦༢༤ ལོར་དུས་ཡུན་ཐུང་ངུ་ཞིག་གི་ནང་འབྲི་གུང་རྫོང་བསྐྱར་བཞེངས་མཛད། ཁོང་གིས་ཕོ་བྲང་གསར་པ་དེར་རྣམ་རྒྱལ་ཆོས་རྫོང་ཞེས་མཚན་གསོལ། འོན་ཀྱང་ཧོ་ཤུད་ཀྱི་དབུ་ཁྲིད་གུ་ཤྲི་ཧན་གྱི་དམག་དཔུང་གིས་དབུས་གཙང་གི་དབང་ཆ་ཏཱ་ལའི་བླ་མ་སྐུ་ཕྲེང་ལྔ་པ་ (༡༦༡༧-༡༦༨༢) ལ་འབུལ་ཆེད་དགྲ་ཕྱོགས་རྣམས་ལ་རྒྱལ་ཁ་ཐག་གཅོད་ཐུབ་པ་ཐོབ་སྟེ། དམག་འཁྲུག་མུ་མཐུད་དུ་བྱུང་། དགེ་ལུགས་པས་རྒྱལ་ཁབ་ཡོངས་ལ་དབང་བསྒྱུར་བྱེད་འགོ་ཚུགས། ཏཱ་ལའི་བླ་མས་བོད་བརྒྱུད་ནང་བསྟན་གྱི་གྲུབ་མཐའ་གཞན་ཁག་གི་སྟེང་དགེ་ལུགས་པའི་མངའ་ཐང་བཙན་གྱིས་བཙུགས་ཤིང་། ཀརྨ་བཀའ་བརྒྱུད་པར་དམིགས་བསལ་གྱི་དྲག་གནོན་བྱས། འབྲི་གུང་གི་གྲོང་གསེབ་གྲངས་ལས་འདས་པ་རྩ་མེད་དུ་སོང་སྟེ་མིང་ཙམ་ལས་མ་ལྷག དགོན་པ་དང་སྐུ་དྲག་གི་བཞུགས་གནས་རྣམས་སོག་དམག་གི་བཅོམ་ཕྲོག་གི་གཞིར་གྱུར་པ་དང་། གསར་དུ་བཞེངས་པའི་རྣམ་རྒྱལ་ཆོས་རྫོང་ཡང་དེའི་ནང་ཚུད།",
      "p2": "རྩ་གཏོར་དང་གྱང་གོག་ཏུ་གྱུར་པའི་དུས་སྐབས་དེའི་ཁྲོད་དུ། འབྲི་གུང་ནི་མི་རྣམས་ཀྱིས་ཡིད་སྨོན་དང་འཇིགས་སྣང་སྐྱེ་བའི་མཐུ་གཏད་ཀྱི་ལྟེ་གནས་ཤིག་ཏུ་གྱུར་པའི་གྲགས་པ་རྒྱང་རིང་པོའི་བར་དུ་ཁྱབ། གྲགས་པ་དེ་ནི་དཀོན་མཆོག་རིན་ཆེན་གྱི་སྤུན་མཆེད་ཆུང་ཚང་རིན་པོ་ཆེ་སྐུ་ཕྲེང་དང་པོ་རིག་འཛིན་ཆོས་གྲགས་ཀྱི་མཛད་ཕྲིན་ལས་བྱུང་བ་ཞིག་ཡིན། རིག་འཛིན་ཆོས་གྲགས་ཀྱིས་འབྲི་གུང་དུ་རྩིས་དང་མོ་ཕྱྭའི་སློབ་གྲྭ་གལ་ཆེན་ཞིག་བཙུགས་པ་དང་། ཁོང་ནི་བོད་ཀྱི་གསོ་བ་རིག་པའི་ལུགས་ཆེན་བཞིའི་ཡ་གྱལ་འབྲི་གུང་གསོ་རིག་ལུགས་ཀྱི་སྲོལ་གཏོད་པ་པོ་ཡང་ཡིན།",
      "p3": "ཆེ་ཚང་གཉིས་པ་དཀོན་མཆོག་ཕྲིན་ལས་བཟང་པོ་ (༡༦༥༦-༡༧༡༨) ཡི་སྐབས་སུ། སྐྱབས་མགོན་རིན་པོ་ཆེ་རྣམ་གཉིས་ཀྱི་ཁྲི་འདོན་མཛད་སྒོ་ཐོག་མར་འབྲི་གུང་རྩེ་དགོན་དུ་གནང་སྲོལ་བཙུགས། ཕྲིན་ལས་བཟང་པོས་བོད་ཀྱི་རི་མོའི་སྲོལ་ཆེན་བཞིའི་ཡ་གྱལ་འབྲི་གུང་འབྲི་རིས་ཞེས་པ་དེ་གསར་གཏོད་མཛད། ༡༦༧༧ ཤིང་སྦྲུལ་ལོར་ཁོང་གིས་འབྲི་གུང་རྩེའི་གཡུལ་ཁའི་སྟེང་དུ་སྦྲུལ་ལོའི་ཆོས་འཁོར་གྱི་སྲོལ་དེ་འགོ་བཙུགས་ཤིང་། དེར་བདེ་མཆོག་དང་གསང་བ་འདུས་པའི་དབང་དང་གདམས་པ་གནང་། ༡༦༨༡ ལོར་ཁོང་གིས་ཡང་རི་སྒར་དགོན་པ་རྩ་བའི་ཆ་ནས་དམག་འཁྲུག་གིས་གཏོར་བཤིག་ཚབས་ཆེན་ཕོག་པས་དེ་ཉིད་ཡོངས་རྫོགས་བསྐྱར་བཞེངས་མཛད། དེང་སང་ཁོང་ནི་དགོན་པ་དེའི་ཕྱག་འདེབས་པར་ངོས་འཛིན་ཞུ། ༡༧༡༧ ལོར་སོག་པོ་ཇུན་གར་བས་ལྷ་སར་བཙན་འཛུལ་དང་རྙིང་མའི་དགོན་སྡེ་མང་པོ་བསྲེགས་བཅོམ་བྱེད་པའི་ཟང་ཟིང་གི་ཁྲོད་དུ། ཁོང་གིས་འབྲི་གུང་རྫོང་ཉམས་གསོ་གནང་འགོ་བཙུགས།",
      "p4": "ཕྲིན་ལས་བཟང་པོས་དུས་ཡུན་རིང་པོར་གཅིག་པུས་བསྟན་པ་བསྐྱངས་ཏེ། གང་ལགས་ཤེ་ན། ཀརྨ་པ་སྐུ་ཕྲེང་བཅུ་པས་ངོས་འཛིན་གནང་བའི་ཆུང་ཚང་གི་ཡང་སྲིད་དེ་འབྲི་གུང་དུ་གདན་འདྲེན་མ་ཞུས་གོང་ནས་ལྷ་འབྲུམ་གྱི་ནད་ཡམས་ཀྱིས་གཤེགས། དེར་བརྟེན་ཆུང་ཚང་གི་ཡང་སྲིད་ཕྲིན་ལས་དོན་གྲུབ་ཆོས་རྒྱལ་ (༡༧༠༤-༡༧༥༤) ཕེབས་པར་ ༡༧༠༤ བར་དུ་འགོར་བ་ཡིན། དོན་གྲུབ་ཆོས་རྒྱལ་ནི་འབྲི་གུང་བནྡེ་དྷརྨ་རཱ་ཛ་ཞེས་པའི་མཚན་གྱིས་ཡོངས་སུ་གྲགས་ལ། དགོན་པ་གསར་པ་འགའ་ཞིག་ཕྱག་བཏབ། ཁོང་གི་གསུང་རྩོམ་བསླབ་བྱ་ནོར་བུའི་གཏེར་མཛོད་ (Jewel Treasury of Advice) ཅེས་པའི་ནང་། མདོ་དང་སྔགས་གཉིས་ཀའི་ལུགས་ལྟར་ནང་ཆོས་ཉམས་ལེན་གྱི་བརྗོད་དོན་ཆ་ཚང་གཅིག་ཏུ་བསྡུས་ཡོད།",
      "p5": "ཆེ་ཚང་གསུམ་པ་དཀོན་མཆོག་བསྟན་འཛིན་འགྲོ་འདུལ་ (༡༧༢༤-༡༧༦༦) གྱི་སྐུ་ཚེ་ནི་ཁོང་གི་དགོན་སྡེ་ཁག་གི་ནང་དུ་རྩོད་གླེང་གིས་ཁྱབ་པས་གྲིབ་འོག་ཏུ་ཚུད། ཁོང་གིས་ཆེད་མངགས་ཕྱི་ཡི་མཛད་པ་དོར་ནས་དུས་ཚོད་མང་ཆེ་བ་ཁྲོ་ལུང་ཕོ་བྲང་གི་གཟིམ་ཆུང་དུ་སྒོམ་སྒྲུབ་ལ་བཞུགས། བྱང་ཡུལ་གྱི་སྐུ་དྲག་གི་ཁྱིམ་རྒྱུད་ནས་ཕེབས་པའི་ཆུང་ཚང་གསུམ་པ་བསྟན་འཛིན་ཆོས་ཀྱི་ཉི་མ་ (༡༧༥༥-༡༧༩༢) ཡིས་དགོན་སྡེ་ཉམས་གསོ་དང་སྒྲིག་ལམ་དག་ཐེར་བྱེད་པར་འབད་བརྩོན་ཆེན་པོ་གནང་ཡང་གནས་སྟངས་དེ་ལས་འགྱུར་ལྡོག་ཆེན་པོ་གཏོང་མ་ཐུབ། ཆེ་ཚང་བཞི་པ་བསྟན་འཛིན་པདྨ་རྒྱལ་མཚན་ (༡༧༧༠-༡༨༢༦) ནི་འབྲི་གུང་བཀའ་བརྒྱུད་ཀྱི་གསེར་ཕྲེང་བླ་མ་རྣམས་ཀྱི་རྣམ་ཐར་གྱི་རྩོམ་པ་པོའི་ཚུལ་དུ་སྙན་གྲགས་ཐོབ། ཆུང་ཚང་བཞི་པ་བསྟན་འཛིན་ཆོས་ཀྱི་རྒྱལ་མཚན་ (༡༧༩༣-༡༨༢༦) དང་པདྨ་རྒྱལ་མཚན་གཉིས་ལོ་གཅིག་ཏུ་དགོངས་པ་རྫོགས་པས། ལྷོ་ཆེན་ཆོས་ཀྱི་བློ་གྲོས་ (༡༨༠༡-༡༨༥༩) ཀྱིས་རྒྱལ་ཚབ་མཛད་དེ་བསྟན་པ་སྐྱོང་དགོས་བྱུང་།",
      "p6": "ཆུང་ཚང་ལྔ་པ་དཀོན་མཆོག་ཆོས་ཉིད་ནོར་བུ་ (༡༨༢༧-༡༨༦༥) དང་། ཆེ་ཚང་ལྔ་པ་དཀོན་མཆོག་ཐུགས་རྗེ་ཉི་མ་ (༡༨༢༨-༡༨༨༥) གཉིས་འབྲི་གུང་མཐིལ་གྱི་ཉེ་འགྲམ་དུ་ཡོད་པའི་འཇིག་རྟེན་གསུམ་མགོན་གྱི་ས་ཁྲི་དམར་པོའི་སྟེང་དུ་དུས་གཅིག་ལ་ཁྲི་འདོན་མཛད། ཁོང་རྣམ་གཉིས་ཀྱི་དབུ་ཁྲིད་ཀྱི་དུས་སྐབས་སུ། སིག་དམག་གིས་ལ་དྭགས་བཙན་བཟུང་བྱས་ཤིང་མངའ་རིས་ཀྱི་ཞིང་ཆེན་གསུམ་བཟུང་། དམག་འཁྲུག་དེ་དག་གི་རིང་ལ་དྭགས་དང་གངས་ཏི་སེའི་ཕྱོགས་ཀྱི་འབྲི་གུང་དགོན་པ་རྣམས་ལ་གོད་ཆག་ཚབས་ཆེན་ཕོག དེ་ལྟ་ནའང་དུས་སྐབས་འདིར་འབྲི་གུང་བསྟན་པ་ཧ་ཅང་དར་རྒྱས་བྱུང་། ཐུགས་རྗེ་ཉི་མ་ནི་མཁས་པ་ཆེན་པོ་ཞིག་དང་། ལྷག་པར་དུ་གསོ་བ་རིག་པའི་ཐད་ལ་མཁས་པར་བརྩི་མོད། འོན་ཀྱང་དགོན་པའི་ལས་བྱེད་པའི་གཡོ་ཇུས་ཀྱི་ནང་དུ་ཚུད། མཐར་ཆེ་ཚང་ཐུགས་རྗེ་ཉི་མ་བོད་གཞུང་གིས་ཐེ་བྱུས་བྱས་པ་བརྒྱུད་ ༡༨༥༤ ལོར་གསེར་ཁྲི་ལས་འབབ་དགོས་བྱུང་། ཉེས་ཆད་ཀྱི་ཚུལ་དུ་འབྲི་གུང་མཐིལ་གྱི་གཙུག་ལག་ཁང་གི་གསེར་གྱི་རྒྱ་ཕིབས་ལྷ་སར་འོར་འདྲེན་བྱས་པ་དང་། འབྲི་གུང་བཀའ་བརྒྱུད་པའི་གཞིས་ཀ་འགའ་ཞིག་གཞུང་བཞེས་བཏང་བས། འབྲི་གུང་དུ་བཟའ་བཅའི་དཀའ་ངལ་ཚབས་ཆེན་བྱུང་། ཐུགས་རྗེ་ཉི་མ་གཙང་དུ་བཙན་བྱོལ་ལ་བཞུགས་རིང་གསང་བའི་ཐོག་གངས་ཏི་སེར་གནས་སྐོར་མཛད་པ་དང་། ས་གནས་གཞན་དག་ཏུ་དབང་ལུང་འཆད་ཁྲིད་གནང་། ལོ་ལྔའི་རྗེས་སུ་ཁོང་བོད་གཞུང་གིས་ཆོག་མཆན་ཐོབ་སྟེ་ཕྱིར་འབྲི་གུང་དུ་ཕེབས་ཐུབ་པ་བྱུང་།",
      "p7": "༡༨༦༥ ལོར་ཆུང་ཚང་ཆོས་ཉིད་ནོར་བུ་དགོངས་པ་རྫོགས་སྐབས། དགོན་པའི་ལས་བྱེད་པ་ཚོས་སླར་ཡང་ཐུགས་རྗེ་ཉི་མར་ངོ་རྒོལ་གྱི་གཡོ་ཇུས་བཤམས་འགོ་བཙུགས། ཐུགས་རྗེ་ཉི་མ་ཡང་བསྐྱར་འབྲི་གུང་མཐིལ་གྱི་གོ་གནས་ནས་ཕྱིར་ཐོན་དགོས་བྱུང་སྟེ་ཁོང་གི་སྐུ་ཚེ་ལྷག་མ་རྣམས་ཁྲོ་ལུང་དུ་བཞུགས་དགོས་བྱུང་། ཁོང་གི་རྗེས་འཇུག་པ་མང་པོ་སྡུག་བསྔལ་གྱི་རྒྱ་མཚོར་བྱིིང་ཡང་། ཁོང་གིས་བྱང་ཆུབ་ཀྱི་སེམས་དང་ཞི་བདེ་ཆེན་པོས་བར་ཆད་ཐམས་ཅད་དང་ལེན་མཛད། ཐུགས་རྗེ་ཉི་མ་སྐུ་ཚེ་གཅིག་ལ་དགོན་པའི་དབང་ཆའི་འཐབ་རྩོད་དང་། མཚན་ཤས་ཀྱི་གྱོད་གཞིའི་འོག་ཐེངས་གཉིས་བྲུལ་དགོས་བྱུང་བ་ནི། འབྲི་གུང་བཀའ་བརྒྱུད་ཀྱི་ལོ་རྒྱུས་ནང་བློ་ཕམ་ཆེ་ཤོས་ཀྱི་ལེའུ་ཞིག་ཡིན་པར་ཐེ་ཚོམ་མེད།",
      "p8": "ཆུང་ཚང་དྲུག་པ་བསྟན་འཛིན་ཆོས་ཀྱི་བློ་གྲོས་ (༡༨༦༨-༡༩༠༦) ནི་སྐུ་གཟུགས་རིང་ཞིང་བརྗིད་ཉམས་ལྡན་པ་ཞིག་ཡོད། ཁོང་གིས་གངས་ཏི་སེ་དང་ལ་ཕྱིའི་སྐོར་གནས་ཡིག་ཆ་ཚང་གཉིས་བརྩམས། ཁོང་གི་མཁས་པའི་བརྩམས་ཆོས་གཉིས་ཀྱིས་བོད་བརྒྱུད་ནང་བསྟན་གྱི་ནང་ཆོས་ལུགས་ཀྱི་ས་ཁམས་རིག་པའི་ལྟ་ཚུལ་ལ་གོ་བ་གཏིང་ཟབ་སྦྱིན་གྱི་ཡོད། ཁོང་གངས་ཏི་སེའི་ཕྱོགས་སུ་བཞུགས་རིང་། ལ་དྭགས་ཤར་ཕྱོགས་སུ་ཕུན་ཚོགས་གླིང་དགོན་པ་ཕྱག་བཏབ་པ་དང་། རྟོགས་ལྡན་རིན་པོ་ཆེ་སྐུ་ཕྲེང་དགུ་པ་མང་ཡུལ་ (ལ་དྭགས་) གྱི་འབྲི་གུང་དགོན་ཁག་གི་དབུ་འཛིན་དུ་ངོས་འཛིན་མཛད། ཁོང་དང་ཆེ་ཚང་དྲུག་པ་བསྟན་འཛིན་ཞི་བའི་བློ་གྲོས་ (༡༨༨༦-༡༩༤༣) རྣམ་གཉིས་ ༡༨༩༣ ལོར་ལྷ་སར་ཕེབས་སྐབས། ༸སྐྱབས་མགོན་རྣམ་གཉིས་ལ་མན་ཇུ་གོང་མས་ཧོ་ཐོག་ཐུའི་མཚན་གནས་ཕུལ། དེ་ནས་བཟུང་འབྲི་གུང་སྐྱབས་མགོན་རིན་པོ་ཆེ་རྣམ་པས་གཞུང་འབྲེལ་གྱི་ཕྱོགས་ཕེབས་སྐབས་དབུ་ཞྭ་གསེར་པོ་ཧོ་ཐོག་ཐུའི་ཞྭ་མོ་བཞེས་སྲོལ་ཡོད་པ་དེ་ནི། སྔོན་ཆུང་ཚང་དང་པོ་རིག་འཛིན་ཆོས་གྲགས་ཀྱིས་མ་འོངས་པར་ཁོང་གིས་གསེར་ཞྭ་བཞེས་པར་འགྱུར་ཞེས་ལུང་བསྟན་པ་དེ་དང་མཐུན་པ་ཡིན།",
      "p9": "ཞི་བའི་བློ་གྲོས་ཀྱི་ཐུགས་མོས་གཙོ་བོ་ནི་སྒོམ་སྒྲུབ་ཀྱི་ཉམས་ལེན་དང་ལྟ་གྲུབ་ཀྱི་གཞུང་གཉིས་ཟུང་དུ་འབྲེལ་རྒྱུ་དེ་ཡིན། གང་ལགས་ཤེ་ན། དེ་གཉིས་ནི་འབྲི་གུང་བཀའ་བརྒྱུད་ཀྱི་སློབ་གསོ་དང་སྦྱོང་བརྡར་གྱི་ཀ་བ་གཙོ་བོ་ཡིན་པས་སོ། ཁོང་ལ་མངོན་ཤེས་ཀྱི་ནུས་པ་ཡོད་པའི་སྐོར་ལ་ཡང་སྙན་གྲགས་ཆེན་པོ་ཡོད།",
      "p10": "ཁོང་གིས་ཡང་རི་སྒར་དགོན་པ་ཉམས་གསོ་དང་། ནང་ཆོས་པར་སྐྲུན་ཆེད་ཤིང་པར་འཇོག་ཡུལ་གྱི་ཁང་པ་ཞིག་གསར་དུ་བརྒྱབ་པ་སོགས་མཛད། མ་ཟད་འབྲི་གུང་མཐིལ་དུ་དགོན་པའི་འཛིན་སྐྱོང་ཡར་རྒྱས་གཏོང་ཆེད་ཚོགས་ཆུང་དང་པོ་དེ་བཙུགས། འོན་ཀྱང་ཁོང་གི་དགོན་སྡེ་ཁག་གི་ཤེས་ཡོན་གནས་ཚད་དེ་ཁོང་གི་ཐུགས་ཁྲལ་ཆེ་ཤོས་ཤིག་ཏུ་གྱུར། ༡༩༣༢ ལོར་ཁོང་གིས་ཉི་མ་ལྕང་ར་བཤད་གྲྭ་བཙུགས།",
      "p11": "ཆུང་ཚང་བདུན་པ་བསྟན་འཛིན་ཆོས་ཀྱི་འབྱུང་གནས་ (༡༩༠༩-༡༩༤༠) དུས་མིན་དགོངས་པ་རྫོགས་རྗེས། ཞི་བའི་བློ་གྲོས་ཐུགས་ཕམ་ཆེན་པོ་བྱུང་ཞིང་། དེའི་རྗེས་སུ་ཁམས་ཕྱོགས་སུ་ཕེབས་སྐབས་སྐུ་གཟུགས་ལ་བསྙུང་གཞི་ཕོག་སྟེ་དྲག་སྐྱེད་མ་བྱུང་། ཁོང་གིས་སྐུ་ཚེ་མ་རྫོགས་བར་དུ་དུས་ཚོད་མང་ཆེ་བ་སྒོམ་སྒྲུབ་ལ་བཞུགས། ཞི་བའི་བློ་གྲོས་ ༡༩༤༣ ལོར་དགོངས་པ་རྫོགས། དེ་ནས་བཟུང་ཁྲི་ཚབ་རྒྱལ་བྲ་ (༡༩༢༤-༡༩༧༩) ཡིས་རྒྱལ་ཚབ་མཛད། ཁོང་གི་དབུ་ཁྲིད་འོག་ད་ལྟའི་ཆུང་ཚང་སྐུ་ཕྲེང་བརྒྱད་པ་དང་ཆེ་ཚང་རིན་པོ་ཆེ་སྐུ་ཕྲེང་བདུན་པ་རྣམ་གཉིས་ཀྱི་ཡང་སྲིད་རྙེད་སོན་བྱུང་སྟེ། གདན་རབས་སོ་དྲུག་པ་དང་སོ་བདུན་པར་ཁྲི་འདོན་མཛད།"
    }
